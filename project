    public static String caeserify(String strEncrypt, int shiftValueKey) {
        Scanner input = new Scanner(System.in);
	    int start = 0;
	    if (shift < 0) {
			start = (int) 'Z' + shift + 1;
	    } 
		else {
			start = 'A' + shift;
	    }
	    String result = "";
	    char currChar = (char) start;
	    for(; currChar <= 'Z'; ++currChar) {
		result = result + currChar;
	    }
	    if(result.length() < 26) {
			for(currChar = 'A'; result.length() < 26; ++currChar) {
		    	result = result + currChar;
		}
	    }
	    return result;
}
        
